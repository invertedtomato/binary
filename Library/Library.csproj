<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>InvertedTomato.IntegerCompression</AssemblyName>
    <RootNamespace>InvertedTomato</RootNamespace>
    <PackageLicenseUrl>https://raw.githubusercontent.com/invertedtomato/integer-compression/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/invertedtomato/integer-compression</PackageProjectUrl>
    <RepositoryUrl>https://github.com/invertedtomato/integer-compression</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Compression VLQ Elias Omega Gamma Delta Fibonacci Integer Numbers</PackageTags>
    <Version>1.2.3</Version>
    <Description>Library for compressing integers in the minimal number of bits. Includes VLQ, Elias Omega, Elias Gamma, Elias Delta, Fibonacci and others.</Description>
    <Copyright>Inverted Tomato</Copyright>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Inverted Tomato</Authors>
    <Company>Inverted Tomato</Company>
    <AssemblyVersion>4.0.0</AssemblyVersion>
    <FileVersion>4.0.0</FileVersion>
    <PackageVersion>4.0.0</PackageVersion>
    <Title>InvertedTomato.IntegerCompression</Title>
    <PackageReleaseNotes>Removed legacy interfaces. Removed dependancy on external library.</PackageReleaseNotes>
    <LangVersion>11</LangVersion>
    <Nullable>enable</Nullable>
    <TargetFrameworks>net7.0;netstandard1.0;netstandard2.0</TargetFrameworks>
  </PropertyGroup>

</Project>